{
  "name": "@hokulea/ember-actions",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "author": "gossi",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input --cache",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "1.8.4",
    "@hokulea/actions": "*",
    "@hokulea/ember-foundation": "*",
    "@hokulea/tokens": "*",
    "ember-element-helper": "^0.6.1"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "^7.20.7",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glint/core": "^1.0.0-beta.3",
    "@glint/environment-ember-loose": "^1.0.0-beta.3",
    "@glint/environment-ember-template-imports": "^1.0.0-beta.3",
    "@glint/template": "^1.0.0-beta.3",
    "@hokulea/actions": "*",
    "@hokulea/layouts": "*",
    "@hokulea/config-eslint": "*",
    "@hokulea/config-prettier": "*",
    "@hokulea/config-stylelint": "*",
    "@hokulea/config-targets": "*",
    "@hokulea/config-template-lint": "*",
    "@hokulea/config-postcss": "*",
    "@hokulea/config-typescript": "*",
    "@hokulea/ember-asset-loader": "*",
    "@hokulea/ember-template-imports": "*",
    "@hokulea/rollup-plugin-postcss": "*",
    "@storybook/addon-actions": "^6.5.15",
    "@types/css-modules": "^1.0.2",
    "babel-plugin-ember-template-compilation": "^2.0.0",
    "concurrently": "^7.6.0",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-source": "^4.11.0",
    "ember-template-imports": "^3.4.1",
    "ember-template-lint": "^5.6.0",
    "eslint": "^8.35.0",
    "lightningcss": "^1.19.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rollup": "^3.18.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-ts": "^3.2.0",
    "storybook-addon-designs": "^6.3.1",
    "stylelint": "^15.2.0",
    "typescript": "~4.9.5",
    "webpack": "^5.75.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react"
      ]
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/button-builder.js": "./dist/_app_/components/button-builder.js",
      "./components/button.js": "./dist/_app_/components/button.js",
      "./components/button2.js": "./dist/_app_/components/button2.js"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./addon-main.js": "./addon-main.js"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
