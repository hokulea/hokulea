{
  "name": "@hokulea/theme-vaomatua",
  "version": "0.0.0",
  "description": "Vaomatua theme for hokulea",
  "license": "MIT",
  "keywords": [
    "theemo-theme"
  ],
  "theemo": {
    "name": "vaomatua",
    "colorSchemes": [
      "light",
      "dark"
    ],
    "file": "dist/vaomatua.css"
  },
  "scripts": {
    "clean": "rm -rf build/ dist/ properties/",
    "sync": "yarn theemo sync",
    "build": "yarn theemo build",
    "generate": "yarn theemo generate",
    "magic": "yarn sync && yarn build && yarn generate",
    "clean-magic": "yarn clean && yarn magic"
  },
  "devDependencies": {
    "style-dictionary": "^3.0.2",
    "theemo": "^0.0.9"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
