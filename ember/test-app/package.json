    "qunit-dom": "^3.0.0",
    "sinon": "^17.0.1",
    "stylelint": "^16.2.1",
    "tracked-built-ins": "^3.3.0",
    "typescript": "~5.3.3",
    "webpack": "^5.90.1"
  },
  "engines": {
    "node": ">= 20.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/setup-ast-transforms"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
