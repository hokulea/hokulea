{
  "name": "@hokulea/theme-moana",
  "version": "0.0.0",
  "description": "Moana theme for hokulea",
  "license": "MIT",
  "keywords": [
    "theemo-theme"
  ],
  "theemo": {
    "name": "moana",
    "colorSchemes": [
      "light",
      "dark"
    ],
    "file": "dist/moana.css"
  },
  "scripts": {
    "clean": "rm -rf build/ dist/ tokens/",
    "sync": "yarn theemo sync",
    "build": "npm-run-all build:*",
    "build:base": "yarn style-dictionary build --config sd.base.js",
    "build:light": "yarn style-dictionary build --config sd.light.js",
    "build:dark": "yarn style-dictionary build --config sd.dark.js",
    "generate": "yarn theemo generate",
    "magic": "npm-run-all sync build generate",
    "clean-magic": "yarn clean && yarn magic"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "style-dictionary": "^3.0.2",
    "theemo": "^0.1.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}