    "@embroider/core": "2.1.1-unstable.72d5a8e",
    "@embroider/test-setup": "2.1.1-unstable.72d5a8e",
    "@embroider/webpack": "2.1.1-unstable.72d5a8e",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@hokulea/config-eslint": "*",
    "@hokulea/config-postcss": "*",
    "@hokulea/config-prettier": "*",
    "@hokulea/config-stylelint": "*",
