{
  "name": "aria-navigator",
  "description": "",
  "author": "gossi",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "tsup --watch",
    "_start": "concurrently -g 'npm:start:*'",
    "start:js": "pnpm build:js -w",
    "start:types": "pnpm build:types -w",
    "build": "tsup",
    "_build": "concurrently -g 'npm:build:*'",
    "build:js": "swc src -d dist",
    "build:types": "tsc -d --emitDeclarationOnly",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@gossi/config-eslint": "^0.7.0",
    "@gossi/config-prettier": "^0.7.0",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.6",
    "@types/eslint": "^8.56.5",
    "@types/lodash.isequal": "^4.5.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">= 18.*"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
