    "ember-resolver": "^10.0.0",
    "ember-source": "~4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.6.0",
    "ember-theemo": "^0.1.3",
    "ember-try": "^2.0.0",
    "eslint": "^8.35.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.0",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "typescript": "~4.9.5",
    "webpack": "^5.75.0"
