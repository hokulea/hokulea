    "@hokulea/config-template-lint": "*",
    "@hokulea/config-typescript": "*",
    "@hokulea/ember-asset-loader": "*",
    "@hokulea/theme-moana": "*",
    "@types/qunit": "^2.19.4",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli": "~4.10.0",
    "ember-cli-app-version": "^6.0.0",
    "ember-cli-babel": "^7.26.11",
