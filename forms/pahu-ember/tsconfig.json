{
  "extends": "@ember/app-tsconfig",
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  },
  "include": [
    "app",
    "src/**/*",
    "tests/**/*",
    "types/**/*"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "#app/*": [
        "./app/*"
      ],
      "#src": [
        "./src/index.ts"
      ],
      "#src/*": [
        "./src/*"
      ],
      "#test-support": [
        "./src/test-support/index.ts"
      ],
      "#test-support/*": [
        "./src/test-support/*"
      ],
      "#tests/*": [
        "./tests/*"
      ]
    },
    "types": [
      "ember-source/types",
      "vite/client",
      "@embroider/core/virtual",
      "qunit-dom"
    ]
  }
}