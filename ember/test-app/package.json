{
  "name": "@hokulea/ember-test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Testing @hokulea/ember",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "dev:test-app": "ember serve -e test -p 4300",
    "dev:package": "pnpm run --filter='@hokulea/ember' start",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:css": "stylelint 'app/**/*.css' --allow-empty-input --cache",
    "lint:css:fix": "stylelint 'app/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "ember test -tp 0"
  },
  "dependencies": {
    "@hokulea/core": "workspace:*",
    "@hokulea/ember": "workspace:*",
    "@hokulea/theme-moana": "workspace:*",
    "@hokulea/tokens": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/compat": "^3.4.4",
    "@embroider/core": "^3.4.4",
    "@embroider/broccoli-side-watch": "0.0.2-unstable.ba9fd29",
    "@embroider/test-setup": "^3.0.3",
    "@embroider/webpack": "^3.2.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.3.0",
    "@glint/environment-ember-loose": "^1.3.0",
    "@glint/environment-ember-template-imports": "^1.3.0",
    "@glint/template": "^1.3.0",
    "@hokulea/config-eslint": "workspace:*",
    "@hokulea/config-prettier": "workspace:*",
    "@hokulea/config-stylelint": "workspace:*",
    "@hokulea/config-targets": "workspace:*",
    "@hokulea/config-template-lint": "workspace:*",
    "@hokulea/config-typescript": "workspace:*",
    "@types/qunit": "^2.19.9",
    "@types/sinon": "^17.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.2",
    "ember-a11y-testing": "^6.1.1",
    "ember-cli": "~5.7.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-command": "^2.0.2",
    "ember-css-modules": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-element-helper": "^0.8.5",
    "ember-link": "^3.2.0",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-polaris-routing": "^0.3.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-set-helper": "^3.0.1",
