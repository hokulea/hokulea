{
  "extends": "@ember/app-tsconfig",
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  },
  "include": [
    "src/**/*",
    "tests/**/*",
    "types/**/*"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "types": [
      "ember-source/types"
    ]
  }
}