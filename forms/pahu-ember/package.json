{
  "name": "@hokulea/pahu-ember",
  "version": "0.0.0",
  "description": "Pahu integration for Ember",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "repository": "https://github.com/hokulea/hokulea",
  "keywords": [
    "ember-addon"
  ],
  "files": [
    "addon-main.cjs",
    "declarations",
    "dist"
  ],
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#src": "./src/index.ts",
    "#src/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "scripts": {
    "build": "rollup --config",
    "dev": "vite dev",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input --cache",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "rollup --config --watch",
    "_test:dev": "vitest"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.6",
    "ember-resources": "^7.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/library-tsconfig": "^1.1.3",
    "@ember/test-helpers": "5.2.2",
    "@embroider/addon-dev": "^8.1.0",
    "@embroider/compat": "^4.1.0",
    "@embroider/core": "^4.1.1",
    "@embroider/macros": "^1.18.0",
    "@embroider/vite": "^1.1.5",
    "@glint/core": "^2.0.0-alpha.3",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.3",
    "@glint/template": "^1.6.0-alpha.2",
    "@gossi/config-eslint": "^1.3.0",
    "@gossi/config-prettier": "^1.1.0",
    "@hokulea/pahu": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "2.19.12",
    "@types/sinon": "^17.0.4",
    "dot-prop": "^9.0.0",
    "ember-modifier": "4.2.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-template-imports": "4.3.0",
    "eslint": "^9.31.0",
    "eslint-plugin-ember": "^12.5.0",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "rollup": "4.45.0",
    "sinon": "^21.0.0",
    "testem": "^3.16.0",
    "typescript": "5.9.2",
    "vite": "^7.0.5"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  }
}
