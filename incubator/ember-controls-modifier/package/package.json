{
  "name": "ember-controls-modifier",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "author": "gossi",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input --cache",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --bundleConfigAsCjs --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "lodash.isequal": "^4.5.0",
    "ember-modifier": "^4.0.0",
    "aria-navigator": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-syntax-decorators": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@gossi/config-eslint": "^0.3.0",
    "@gossi/config-prettier": "^0.3.0",
    "@gossi/config-targets": "^0.3.0",
    "@gossi/config-template-lint": "^0.3.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/lodash.isequal": "^4.5.6",
    "concurrently": "^7.6.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-source": "~4.9.3",
    "ember-template-lint": "^5.3.0",
    "eslint": "^8.31.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "rollup": "^3.9.1",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "~4.8.4",
    "webpack": "^5.75.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./modifiers/control.js": "./dist/_app_/modifiers/control.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "engines": {
    "node": ">= 16.*"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
