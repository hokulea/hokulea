{
  "name": "@hokulea/ember-blueprint",
  "version": "0.0.0",
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "author": "gossi",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input --cache",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --bundleConfigAsCjs --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.3"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/plugin-syntax-decorators": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@embroider/addon-dev": "^2.0.0",
    "@hokulea/config-eslint": "*",
    "@hokulea/config-prettier": "*",
    "@hokulea/config-stylelint": "*",
    "@hokulea/config-targets": "*",
    "@hokulea/config-template-lint": "*",
    "@hokulea/config-typescript": "*",
    "@rollup/plugin-babel": "^6.0.2",
    "@types/css-modules": "^1.0.2",
    "concurrently": "^7.5.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-template-lint": "^5.3.0",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "~4.8.4"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {}
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "engines": {
    "node": ">= 16.*"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
